{
  "name": "vue3-mindmap-editor",
  "description": "Mind map editor  for Vue3",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "main": "./dist/vue3-mindmap.umd.js",
  "module": "./dist/vue3-mindmap.es.js",
  "exports": {
    ".": {
      "import": "./dist/vue3-mindmap.es.js",
      "require": "./dist/vue3-mindmap.umd.js"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "version": "0.5.8",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "lib": "vite build --config vite-lib.config.ts && tsc --project tsconfig.lib.json",
    "serve": "vite preview",
    "test": "jest --watch",
    "coverage": "jest --coverage",
    "release": "standard-version",
    "deploy": "sh deploy.sh"
  },
  "dependencies": {
    "array-to-tree": "^3.3.2",
    "axios": "^0.21.2",
    "d3-drag": "^2.0.0",
    "d3-interpolate": "^2.0.1",
    "d3-scale": "^3.3.0",
    "d3-scale-chromatic": "^2.0.0",
    "d3-selection": "^2.0.0",
    "d3-shape": "^2.1.0",
    "d3-transition": "^2.0.0",
    "d3-zoom": "^2.0.0",
    "html2canvas": "^1.0.0-rc.7",
    "lodash.clonedeep": "^4.5.0",
    "mitt": "^2.1.0",
    "mobile-device-detect": "^0.4.3",
    "vue": "^3.2.20",
    "vue-mobile-detection": "^1.0.0"
  },
  "devDependencies": {
    "@types/d3-drag": "^2.0.0",
    "@types/d3-ease": "^2.0.0",
    "@types/d3-scale": "^3.2.2",
    "@types/d3-scale-chromatic": "^2.0.0",
    "@types/d3-selection": "^2.0.0",
    "@types/d3-shape": "^2.0.0",
    "@types/d3-transition": "^2.0.0",
    "@types/d3-zoom": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash.clonedeep": "^4.5.6",
    "@types/node": "^15.0.2",
    "@types/resize-observer-browser": "^0.1.5",
    "@types/string-hash": "^1.1.1",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.11",
    "autoprefixer": "^10.3.7",
    "eslint": "^7.26.0",
    "eslint-plugin-tsdoc": "^0.2.14",
    "jest": "^26.6.3",
    "postcss": "^8.3.9",
    "sass": "^1.32.12",
    "standard-version": "^9.3.0",
    "string-hash": "^1.1.3",
    "ts-jest": "^26.5.6",
    "ts-node": "^9.1.1",
    "tslib": "^2.2.0",
    "typescript": "~4.2.4",
    "vite": "^2.3.0"
  },
  "keywords": [
    "mindmap",
    "vue",
    "d3"
  ]
}
